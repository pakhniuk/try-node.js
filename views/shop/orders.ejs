<%- include('../includes/head.ejs') %>
</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <% if(orders.length > 0) {%>
            <ul>
                <% for(let order of orders) { %>
                    <li>
                        <h1># <%= order._id %></h1>
                        <ul>
                            <% for(let item of order.products) { %>
                                <li>
                                    <%= item.product.title %> (<%= item.quantity %>)
                                </li>
                            <% } %>
                        </ul>
                    </li>
                <% } %>
            </ul>
        <% } else { %>
            <h1>Nothing there!</h1>
        <% } %>
    </main>

<%- include('../includes/end.ejs') %>